//first nodejs practice
var http=require('http');
var fs=require('fs'); //fs for reading files on computer

http.createServer(function(req, res){
    fs.readFile('D:\pcode1.html', function(err,data){
    res.writeHead(200, {'content-Type': 'text/plain'});
    res.write(data);
    return res.end();
    });
}).listen(8080);
//reading file
